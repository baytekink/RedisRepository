<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Serkan Yazıcıoğlu</Authors>
    <Company>Serkan Yazıcıoğlu</Company>
    <Description>.Net standard repository library for Redis.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/serkanyazicioglu/RedisRepository</PackageProjectUrl>
    <RepositoryUrl>https://github.com/serkanyazicioglu/RedisRepository</RepositoryUrl>
    <PackageTags>repository, nhea, redis, cache</PackageTags>
    <PackageReleaseNotes>Added connection pooling.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.5</Version>
    <AssemblyVersion>2.0.5.0</AssemblyVersion>
    <FileVersion>2.0.5.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nhea" Version="2.0.0.4" />
    <PackageReference Include="StackExchange.Redis" Version="2.1.58" />
    <PackageReference Include="System.Runtime.Caching" Version="4.7.0" />
    <PackageReference Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

</Project>
