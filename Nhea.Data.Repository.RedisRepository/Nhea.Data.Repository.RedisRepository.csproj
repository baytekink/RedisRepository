<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Serkan Yazıcıoğlu</Authors>
    <Company>Serkan Yazıcıoğlu</Company>
    <Description>A .Net standard library which provides logging, mailing, convertion and base repository classes.</Description>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/serkanyazicioglu/RedisRepository</PackageProjectUrl>
    <RepositoryUrl>https://github.com/serkanyazicioglu/RedisRepository</RepositoryUrl>
    <PackageTags>repository, nhea, redis, cache</PackageTags>
    <PackageReleaseNotes>Minor changes</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nhea" Version="1.5.5" />
    <PackageReference Include="StackExchange.Redis" Version="2.0.519" />
    <PackageReference Include="System.Runtime.Caching" Version="4.5.0" />
  </ItemGroup>

</Project>
